{
  "name": "tutorial-react-parcel-express",
  "version": "1.0.0",
  "description": "A tiny getting started for a react project, with front-end built with parcel, and served through express.",
  "main": "index.js",
  "scripts": {
    "build-watch": "parcel watch ./client/index.html",
    "start-watch": "nodemon server/index.js",
    "dev": "concurrently --kill-others \"nodemon server/index.js\" \"parcel ./client/index.html\"",
    "build": "parcel build ./client/index.html",
    "start": "npm run build && node server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "tutorial",
    "react",
    "parcel",
    "express"
  ],
  "author": "witchard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/witchard/tutorial-react-parcel-express.git"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.12.2",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.3.5"
  },
  "bugs": {
    "url": "https://github.com/witchard/tutorial-react-parcel-express/issues"
  },
  "homepage": "https://github.com/witchard/tutorial-react-parcel-express#readme"
}
